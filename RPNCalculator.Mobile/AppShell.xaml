<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RPNCalculator.Mobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RPNCalculator.Mobile"
    Shell.FlyoutBehavior="Disabled"
    Title="RPNCalculator.Mobile">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Documentation"
        ContentTemplate="{DataTemplate local:DocumentationPage}"
        Route="DocumentationPage" />
</Shell>
